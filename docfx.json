{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "retired/**",
          "tutorials/android/**",
          "tutorials/angular/**",
          "tutorials/aspnet-core/**",
          "tutorials/azure-functions/**",
          "tutorials/blazor/**",
          "tutorials/bot-framework/**",
          "tutorials/dotnet-core/**",
          "tutorials/ios-objectivec/**",
          "tutorials/ios-swift/**",
          "tutorials/java/**",
          "tutorials/javascript/**",
          "tutorials/node/**",
          "tutorials/office-addin/**",
          "tutorials/php/**",
          "tutorials/power-automate/**",
          "tutorials/python/**",
          "tutorials/react/**",
          "tutorials/react-native/**",
          "tutorials/ruby/**",
          "tutorials/spfx/**",
          "tutorials/uwp/**",
          "tutorials/xamarin/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/graph/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "uhfHeaderId": "MSDocsHeader-MSGraph"
    },
    "fileMetadata": {
      "featureFlags": {
        "tutorials/*.yml": [
          "enable_tutorial_feedback"
        ]
      },
      "titleSuffix": {
        "**/*.yml": "Microsoft Graph"
      }
    },
    "template": [],
    "dest": "graph-training",
    "markdownEngineName": "markdig"
  }
}
